// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int      @default(autoincrement()) @id
  email       String   @unique
  username    String   @unique
  password    String
}

generator client {
  provider = "prisma-client-js"
}
